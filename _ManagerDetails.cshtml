


@foreach (var item in ViewBag.leadmanagerItem)
{
    <div class="container-fluid">

        <div class="row">
            <div class="col-12">
                <div class="card">
                    <div class="card-body  met-pro-bg">
                        <div class="met-profile">
                            <div class="row">
                                <div class="col-lg-4 align-self-center mb-3 mb-lg-0">
                                    <div class="met-profile-main">
                                        <div class="met-profile-main-pic">
                                            <img src="@item.PhotoUrl" style="height:100px;width:100px;" class="rounded-circle">
                                            <span class="fro-profile_main-pic-change">
                                                <i class="fas fa-camera"></i>
                                            </span>
                                        </div>
                                        <div class="met-profile_user-detail">
                                            <h5 class="met-user-name">@item.Name</h5>
                                            <p class="mb-0 met-user-name-post">@item.Designation</p>
                                        </div>
                                    </div>
                                </div><!--end col-->
                                <div class="col-lg-4 ml-auto">
                                  
                                    <ul class="list-unstyled personal-detail">
                                        <li class=""><i class="dripicons-phone mr-2 text-info font-18"></i> <b> phone </b> :@item.Phone</li>
                                        <li class="mt-2"><i class="dripicons-mail text-info font-18 mt-2 mr-2"></i> <b> Email </b> : @item.Email</li>
                                        <li class="mt-2"><i class="dripicons-location text-info font-18 mt-2 mr-2"></i> <b>Country</b> : @item.Country</li>
                                    </ul>
                                    <div class="button-list btn-social-icon">
                                        <a href="@item.Facebook" class="btn btn-blue btn-round">
                                            <i class="fab fa-facebook-f"></i>
                                        </a>

                                        <a href="@item.Twitter" class="btn btn-secondary btn-round ml-2">
                                            <i class="fab fa-twitter"></i>
                                        </a>

                                        <a href="@item.LinkdIn" class="btn btn-pink btn-round  ml-2">
                                            <i class="fab fa-dribbble"></i>
                                        </a>
                                    </div>
                                </div><!--end col-->
                            </div><!--end row-->
                        </div><!--end f_profile-->
                    </div><!--end card-body-->

                </div><!--end card-->
            </div><!--end col-->

        </div><!--end row-->

        <div class="row">
            <div class="col-lg-6">
                <div class="card">
                    <div class="card-body">
                        <h4 class="header-title mt-0 mb-3">Education</h4>
                        <div class="slimscroll education-activity">
                            <div class="activity">
                                <i class="mdi mdi-school icon-success"></i>
                                <div class="time-item">
                                    <div class="item-info">
                                        <div class="d-flex justify-content-between align-items-center">
                                            <h6 class="m-0">University</h6>
                                            <span class="text-muted">Oct-2009 to Oct-2011</span>
                                        </div>
                                        <p class="text-muted mt-3">
                                           @item.Bsc
                                        
                                        </p>
                                        <div>
                                            <span class="badge badge-soft-secondary">Design</span>
                                            <span class="badge badge-soft-secondary">HTML</span>
                                            <span class="badge badge-soft-secondary">Css</span>

                                        </div>
                                    </div>
                                </div>
                                <i class="mdi mdi-medal icon-pink"></i>
                                <div class="time-item">
                                    <div class="item-info">
                                        <div class="d-flex justify-content-between align-items-center">
                                            <h6 class="m-0">HSC</h6>
                                            <span class="text-muted">Oct-2006 to Oct-209</span>
                                        </div>
                                        <p class="text-muted mt-3">
                                          @item.HSC
                                          
                                        </p>
                                        <div>
                                            <span class="badge badge-soft-secondary">Python</span>
                                            <span class="badge badge-soft-secondary">Django</span>
                                        </div>
                                    </div>
                                </div>
                                <i class="mdi mdi-book-open-page-variant icon-purple"></i>
                                <div class="time-item">
                                    <div class="item-info">
                                        <div class="d-flex justify-content-between align-items-center">
                                            <h6 class="m-0">SSC</h6>
                                            <span class="text-muted">Oct-2003 to Oct-2006</span>
                                        </div>
                                        <p class="text-muted mt-3">
                                            @item.SSC
                                          
                                        </p>
                                    </div>
                                </div>
                              
                            </div><!--end activity-->
                        </div><!--end education-activity-->
                    </div>  <!--end card-body-->
                </div><!--end card-->
            </div><!--end col-->

            <div class="col-lg-6">
                <div class="card">
                    <div class="card-body">
                        <h4 class="header-title mt-0 mb-3">My Skills</h4>
                        <div class="row">
                            <div class="col-4">
                                <img src="../assets/images/widgets/education.svg" alt="" class="img-fluid">
                            </div>
                            <div class="col-8 align-self-center">
                                <p class="skill-detail text-justify">
                                   @item.Skill
                                </p>
                            </div>
                        </div>
                       
                    </div>  <!--end card-body-->
                </div><!--end card-->
            </div><!--end col-->

        </div><!--end row-->
    </div><!-- container -->

}

